<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE riflspec SYSTEM "rifl4Java.dtd">
<riflspec>
  <domains>
    <domain name="HIGH"/>
    <domain name="LOW"/>
  </domains>
  <flowrelation>
    <flow from="HIGH" to="LOW"/>
  </flowrelation>
  <domainassigment>
    <assign domain="HIGH" handle="H"/>
    <assign domain="LOW" handle="L"/>
  </domainassigment>
  <assignable handle="H">
    <category name="notavailable">
      <source>
        <field class="edu.kit.iti.rifl.FirstTest" name="high"/>
      </source>
    </category>
  </assignable>
  <assignable handle="L">
    <category name="notavailable">
      <sink>
        <parameter class="edu.kit.iti.rifl.FirstTest" method="in_secure(java.util.List&lt;java.lang.Integer&gt;)" parameter="i"/>
      </sink>
      <sink>
        <returnvalue class="edu.kit.iti.rifl.FirstTest" method=""/>
      </sink>
    </category>
  </assignable>
</riflspec>
